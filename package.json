{
  "name": "awilix-express",
  "version": "1.0.0",
  "description": "Awilix helpers for Express",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "tslint --project tsconfig.json --fix \"src/**/*.ts\"",
    "cover": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/talyssonoc/awilix-express.git"
  },
  "files": [
    "lib",
    "LICENSE.md",
    "README.md"
  ],
  "directories": {
    "lib": "lib"
  },
  "keywords": [],
  "author": "Talysson <talyssonoc@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talyssonoc/awilix-express/issues"
  },
  "homepage": "https://github.com/talyssonoc/awilix-express",
  "dependencies": {
    "awilix-router-core": "^1.3.2",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/glob": "^5.0.34",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.4.6",
    "@types/rimraf": "^2.0.2",
    "assert-request": "^1.0.6",
    "awilix": "^3.0.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-latest": "^6.24.1",
    "coveralls": "^3.0.0",
    "express": ">=4.0.0",
    "jest": "^22.0.4",
    "rimraf": "^2.6.1",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.0"
  },
  "peerDependencies": {
    "awilix": "^3.0.0",
    "express": ">=4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "mapCoverage": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePaths": [
      "src"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "transform": {
      "\\.(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.(js|jsx)": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {}
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": "transform-decorators-legacy"
  }
}
